#! /bin/sh

. ./utils

exit_status=0

run_ls() {
    run 0 - "ls $*" ../dctc-jar.sh ls "$@" <<EOF
$(ls $@)
EOF
    if $fail; then
        exit_status=1
    fi
}

run_ls /
run_ls /var
run_ls / /var
run_ls /tmp
run_ls /bin
run_ls data/
run_ls data/ls
run_ls /etc/bash.bashrc
run_ls -a data/ls

echo $exit_status
